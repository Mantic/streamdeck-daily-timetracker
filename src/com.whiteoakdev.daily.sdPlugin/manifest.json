{
	"SDKVersion": 2,
	"Author": "Michael Whitaker",
	"CodePath": "app.html",
	"Description": "A simple daily timetracker for the Elgato Stream Deck.",
	"Name": "Stream Deck Daily Timetracker",
	"Icon": "actions/template/assets/action",
	"URL": "https://github.com/Mantic/streamdeck-daily-timetracker",
	"Version": "0.1.0",
	"Software": {
		"MinimumVersion": "5.0"
	},
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "10.11"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		}
	],
	"Category": "Time Management",
	"CategoryIcon": "assets/time-management",
	"Actions": [
		{
			"Icon": "assets/viewlog",
			"Name": "Log Control",
			"Controllers": ["Keypad"],
			"DisableAutomaticStates": true,
			"States": [
				{
					"Image": "assets/viewlogAction"
				}
			],
			"Tooltip": "Views, starts, and stops, the current day's log.",
			"UUID": "com.whiteoakdev.daily.log",
			"PropertyInspectorPath": "actions/log/property-inspector/inspector.html"
		},
		{
			"Icon": "assets/logentry",
			"Name": "Log Entry",
			"Controllers": ["Keypad"],
			"DisableAutomaticStates": true,
			"States": [
				{
					"Image": "assets/logentryAction"
				}
			],
			"Tooltip": "Adds a new log entry to a log.",
			"UUID": "com.whiteoakdev.daily.logentry",
			"PropertyInspectorPath": "actions/logentry/property-inspector/inspector.html"
		}
	]
}
